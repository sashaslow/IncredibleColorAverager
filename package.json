    "babel-preset-latest": "^6.24.1",
    "watch": "^1.0.2",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sashaslow/IncredibleColorAverager.git"
  },
  "bugs": {
    "url": "https://github.com/sashaslow/IncredibleColorAverager/issues"
  },
  "homepage": "https://github.com/sashaslow/IncredibleColorAverager#readme"
