  "description": "Get the average color of your img element in a css-readable format",
  "main": "build/index.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel src -d build"
  },
  "babel": {
    "presets": [
      "latest"
    ]
